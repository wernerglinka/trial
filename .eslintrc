{
    "env": {
        "browser": true,
        "jest": true
    },
    "extends": [
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-one-expression-per-line": 0,
        "class-methods-use-this": 0,
        "import/no-named-as-default": 0,
        "react-hooks/rules-of-hooks": "error",
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    },
    "plugins": [
        "react-hooks"
    ]
}